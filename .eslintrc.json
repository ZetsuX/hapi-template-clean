{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-undef": 0,
    "comma-dangle": 0,
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0
  }
}
